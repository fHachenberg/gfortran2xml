<?xml version="1.0"?>

<library lang="fortran" version="2" name="UNKNOWN.f90">

<!-- include files that the topmost file includes -->
<!-- fortran module scopes -->

<!-- types and functions NOT contained in a scope -->

  <!--
    - MODULE: F90Global
    -->
  <scope name="F90Global" kind="module">

    <!--
      - METHOD: pcpocon
      -->
    <method name="pcpocon" kind="static">

      <!-- ARGUMENT : uplo -->
      <arg name="uplo" intent="inout">
        <type kind="fchar" ikind="char" clen="1"/>
      </arg>

      <!-- ARGUMENT : n -->
      <arg name="n" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- ARGUMENT : a -->
      <arg name="a" intent="inout">
        <type kind="farray">
          <array lang="fortran" rank="1">
            <type kind="fcmplx" fkind="float"/>
            <dimension extent="*"/>
          </array>
        </type>
      </arg>

      <!-- ARGUMENT : ia -->
      <arg name="ia" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- ARGUMENT : ja -->
      <arg name="ja" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- ARGUMENT : desca -->
      <arg name="desca" intent="inout">
        <type kind="farray">
          <array lang="fortran" rank="1">
            <type kind="fint" ikind="int"/>
            <dimension extent="*"/>
          </array>
        </type>
      </arg>

      <!-- ARGUMENT : anorm -->
      <arg name="anorm" intent="inout">
        <type kind="ffloat" fkind="float"/>
      </arg>

      <!-- ARGUMENT : rcond -->
      <arg name="rcond" intent="inout">
        <type kind="ffloat" fkind="float"/>
      </arg>

      <!-- ARGUMENT : work -->
      <arg name="work" intent="inout">
        <type kind="farray">
          <array lang="fortran" rank="1">
            <type kind="fcmplx" fkind="float"/>
            <dimension extent="*"/>
          </array>
        </type>
      </arg>

      <!-- ARGUMENT : lwork -->
      <arg name="lwork" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- ARGUMENT : rwork -->
      <arg name="rwork" intent="inout">
        <type kind="farray">
          <array lang="fortran" rank="1">
            <type kind="ffloat" fkind="float"/>
            <dimension extent="*"/>
          </array>
        </type>
      </arg>

      <!-- ARGUMENT : lrwork -->
      <arg name="lrwork" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- ARGUMENT : info -->
      <arg name="info" intent="inout">
        <type kind="fint" ikind="int"/>
      </arg>

      <!-- return value -->
      <return>
        <type kind="fvoid"/>
      </return>
    </method>

  </scope>

</library>
