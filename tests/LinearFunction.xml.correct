<?xml version="1.0"?>

<library lang="fortran" version="2" name="UNKNOWN.f90">

<!-- include files that the topmost file includes -->
<!-- fortran module scopes -->

  <!--
    - MODULE: linearfunction
    -->
  <scope name="linearfunction" kind="module">

    <!--
      - METHOD: evaluate
      -->
    <method name="evaluate" kind="static">

      <!-- return value -->
      <return>
        <type kind="fvoid"/>
      </return>
    </method>

    <!--
      - METHOD: linearfunction::evaluate_lf
      -->
    <method name="evaluate_lf" kind="static">

      <!-- ARGUMENT : x -->
      <arg name="x" intent="inout">
        <type kind="ffloat" fkind="dbl"/>
      </arg>

      <!-- return value -->
      <return>
        <type kind="ffloat" fkind="dbl"/>
      </return>
    </method>

  </scope>

</library>
